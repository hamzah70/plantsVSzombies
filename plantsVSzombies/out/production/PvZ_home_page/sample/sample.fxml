<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <ImageView disable="true" fitHeight="467.0" fitWidth="540.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@homePage.jpg" />
         </image>
      </ImageView>
      <Button fx:id="Homebtn" mnemonicParsing="false" onAction="#Homebutton" opacity="0.0" prefHeight="316.0" prefWidth="531.0" text="Start Game" />
   </children>
</GridPane>
