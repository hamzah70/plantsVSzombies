<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="401.0" prefWidth="239.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MenuPage">
   <children>
      <ImageView fitHeight="416.0" fitWidth="655.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@mainPage.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="quitGame" fitHeight="46.0" fitWidth="90.0" layoutX="282.0" layoutY="252.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@quit.png" />
         </image>
      </ImageView>
      <Button layoutX="292.0" layoutY="261.0" mnemonicParsing="false" onAction="#quitPress" opacity="0.0" text="Quit" />
      <ImageView fx:id="load" fitHeight="54.0" fitWidth="90.0" layoutX="267.0" layoutY="190.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@load.png" />
         </image>
      </ImageView>
      <Button layoutX="272.0" layoutY="195.0" mnemonicParsing="false" onAction="#LoadPress" opacity="0.0" text="Load Game" />
      <ImageView fx:id="newgameImage" fitHeight="46.0" fitWidth="90.0" layoutX="268.0" layoutY="126.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@newGame.png" />
         </image>
      </ImageView>
      <Button layoutX="273.0" layoutY="130.0" mnemonicParsing="false" onAction="#startPress" opacity="0.0" text="New Game" />
      <ImageView fx:id="level" fitHeight="57.0" fitWidth="79.0" layoutX="273.0" layoutY="157.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@selectLevel.jpg" />
         </image>
      </ImageView>
      <Button fx:id="selectLevel" layoutX="270.0" layoutY="157.0" mnemonicParsing="false" onMouseClicked="#levelSelectOpen" opacity="0.0" text="Select Level" />
      <ImageView fitHeight="93.0" fitWidth="194.0" layoutX="251.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@PvZ_PNGs/frenzy2.png" />
         </image>
      </ImageView>
      <Button fx:id="frenzy" layoutX="261.0" layoutY="49.0" mnemonicParsing="false" onAction="#frenzy" opacity="0.0" prefHeight="27.0" prefWidth="103.0" text="frenzy" />
   </children>
</AnchorPane>
